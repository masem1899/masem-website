{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.masem.at"
        }
      ],
      "destination": "https://masem.at/:path*",
      "statusCode": 308
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(googlebot|bingbot|yandex|baiduspider|facebookexternalhit|twitterbot).*"
        }
      ],
      "destination": "/api/prerender?path=/:path*"
    },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/site.webmanifest", "destination": "/site.webmanifest" },
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/_vercel/:path*", "destination": "/_vercel/:path*" },
    { "source": "/:path*", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(googlebot|bingbot|yandex|baiduspider|facebookexternalhit|twitterbot).*"
        }
      ],
      "headers": [
        {
          "key": "X-Prerender-Token",
          "value": "Nw2V48rcSh7oubSszKPk"
        }
      ]
    }
  ]
}
